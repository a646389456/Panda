<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lenovo.pandas">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Login"
            android:icon="@drawable/panda_11"
            android:launchMode="singleTask">
           <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FirstPass"
            android:icon="@drawable/panda_01"
            android:launchMode="singleTask">
        </activity>
        <activity
            android:name=".SecondPass"
            android:icon="@drawable/panda_02"
            android:launchMode="singleTask">
        </activity>
        <activity
            android:name=".ThridPass"
            android:icon="@drawable/panda_03"
            android:launchMode="singleTask">
        </activity>
        <activity
            android:name=".FourPass"
            android:icon="@drawable/panda_04"
            android:launchMode="singleTask">
        </activity>
        <activity
            android:name=".FivePass"
            android:icon="@drawable/panda_05"
            android:launchMode="singleTask">
        </activity>
    </application>

</manifest>
